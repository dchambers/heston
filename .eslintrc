{
  "extends": "eslint:recommended",
  "env": {
    "es6": true
  },
  "rules": {
    "no-console": 1
  },
  "globals": {
    "console": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "no-implicit-side-effects"
  ]
}
